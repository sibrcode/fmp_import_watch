<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.sibr.fmp_import_watch</string>
	<key>ProgramArguments</key>
	<array>
		<string>~/Library/Application Support/fmp_import_watch.py</string>
	</array>
	<key>EnableGlobbing</key>
		<true/>
	<key>LowPriorityIO</key>
        <true/>
    <key>Nice</key>
    	<integer>20</integer>
	<key>StandardErrorPath</key>
		<string>/tmp/fmp_import_watch_err.log</string>
	<key>StandardOutPath</key>
		<string>/tmp/fmp_import_watch.log</string>
	<key>WatchPaths</key>
	<array>
		<string>~/Documents/Import.log</string>
	</array>
</dict>
</plist>
